    "fetch": "node fetcher/v1/index.js",
    "fetch:v1": "node fetcher/v1/index.js",
    "schedule": "node fetcher/schedule.js",
    "import-units": "node server/v1/scripts/importUnits.js",
    "fetch:v2": "node fetcher/v2/index.js",
    "import-units:v2": "node server/scripts/importUnits.v2.js"
